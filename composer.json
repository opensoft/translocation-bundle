{
    "name": "t73biz/translocation-bundle",
    "description": "Translocation Bundle for Symfony 2",
	"keywords": ["translate", "localization", "internationalization", "extraction"],
	"type": "symfony-bundle",
	"license": "MIT",
	"authors": [
		{
			"name": "Ronald Chaplin",
			"email": "ron.g.chaplin@gmail.com"
		}
	],
    "require": {
        "nikic/php-parser": "0.9.*@dev"
	},
	"require-dev": {
		"twig/twig": "~1.11"
	},
	"autoload": {
		"psr-0" : {
			"T73Biz\\TranslocationBundle" : "src"
		}
	},
	"target-dir": "T73Biz/Bundle/TranslocationBundle",
    "minimum-stability": "dev",
	"extra": {
		"branch-alias": {
			"dev-master": "0.1.x-dev"
		}
	}
}
